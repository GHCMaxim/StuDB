doctype html
head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Course
    link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css' integrity='sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk' crossorigin='anonymous')
    link(href='https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;700&display=swap' rel='stylesheet')
    link(rel='stylesheet' href="{{url_for('static', filename='style.css')}}")
nav.navbar.navbar-expand-sm.navbar-dark.fixed-top(style='background-color: #1b4965;')
    a.navbar-brand(href='index') Student Management
    button.navbar-toggler(data-toggle='collapse' data-target='#expandme')
        span.navbar-toggler-icon
    #expandme.collapse.navbar-collapse
        .navbar-nav
            a.nav-item.nav-link(href='home') Home
            a.nav-item.nav-link(href='student') Student
            a.nav-item.nav-link(href='course') Course
            a.nav-item.nav-link(href='grade') Grade
            a.nav-item.nav-link(href='teacher') Teacher
            a.nav-item.nav-link(href='attendance') Attendance
        form.form-inline.ml-auto
            input.form-control.mr-sm-2(type='search' placeholder='Search' aria-label='Search')
            button.btn.btn-outline-light.my-2.my-sm-0(type='submit') Search
.container
    h1
main.home
    .container-fluid.hero3.d-flex.align-items-center.justify-content-center
        .text-center
            h1.display-4.mb-0.text-primary-emphasis ABOUT COURSE
            h2.display-5.mt-0.text-primary-emphasis Please select an action below
            button.btn.btn-outline-info.btn-lg(onclick="takeAction('create', 'course')") Add Course
            button.btn.btn-outline-info.btn-lg(onclick="takeAction('read', 'course')") View Course
            button.btn.btn-outline-info.btn-lg(onclick="takeAction('update', 'course')") Edit Course
            button.btn.btn-outline-info.btn-lg(onclick="takeAction('delete', 'course')") Delete Course
            button.btn.btn-outline-danger.btn-lg(onclick='goBack()') Back
script(src='https://code.jquery.com/jquery-3.5.1.slim.min.js' integrity='sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj' crossorigin='anonymous')
script(src='https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js' integrity='sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo' crossorigin='anonymous')
script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js' integrity='sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI' crossorigin='anonymous')
script(src="{{url_for('static', filename='app.js')}}")
